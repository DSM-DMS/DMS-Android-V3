<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dsm.android.v3">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name=".util.App"
            android:allowBackup="true"
            android:icon="@mipmap/dms_logo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/dms_logo_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name=".ui.applyMusic.ApplyMusicDomActivity">
        </activity>
        <activity
                android:name=".ui.signIn.SignInActivity"
                android:label="@string/app_name">
        </activity>
        <activity
                android:name=".ui.introduceTeam.IntroDeveloperActivity"
                android:theme="@style/AppTheme"/>
        <activity
                android:name=".ui.splash.SplashActivity"
                android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.main.MainActivity"/>
        <activity android:name=".ui.register.RegisterActivity"/>

        <activity
                android:name=".ui.pointLog.PointLogActivity"
                android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".ui.changePassword.ChangePasswordActivity">
        </activity>
        <activity android:name=".ui.applyGoing.ApplyGoingActivity">
        </activity>
        <activity android:name=".ui.applyGoingLog.ApplyGoingLogActivity"/>
        <activity android:name=".ui.applyGoingDoc.ApplyGoingDocActivity"/>
        <activity android:name=".ui.applyGoingEdit.ApplyGoingEditActivity"/>
        <activity android:name=".ui.applyStaying.ApplyStayingActivity">
        </activity>
        <activity android:name=".ui.applyExtensionStudy.ApplyExtensionStudyActivity">
        </activity>
        <activity android:name=".ui.applyMusic.ApplyMusicActivity"/>
        <activity android:name=".ui.applyMusicLog.ApplyMusicLogActivity"/>
        <activity android:name=".ui.notice.NoticeActivity"/>
    </application>
</manifest>