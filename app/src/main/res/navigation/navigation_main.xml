<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/navigation_main"
            app:startDestination="@id/mealFragment">

    <fragment
            android:id="@+id/mealFragment"
            android:name="dsm.android.v3.ui.main.meal.MealFragment"
            android:label="MealFragment"/>
    <fragment
            android:id="@+id/putInFragment"
            android:name="dsm.android.v3.ui.main.putIn.PutInFragment"
            android:label="PutInFragment"/>
    <fragment
            android:id="@+id/noticeFragment"
            android:name="dsm.android.v3.ui.notice.NoticeFragment"
            android:label="NoticeFragment">
        <action
                android:id="@+id/action_noticeFragment_to_noticeListFragment"
                app:destination="@id/noticeListFragment"/>
    </fragment>
    <fragment
            android:id="@+id/myPageFragment"
            android:name="dsm.android.v3.ui.mypage.MyPageFragment"
            android:label="MyPageFragment"/>
    <fragment
            android:id="@+id/noticeListFragment"
            android:name="dsm.android.v3.ui.notice.NoticeListFragment"
            android:label="NoticeListFragment">
    </fragment>
</navigation>